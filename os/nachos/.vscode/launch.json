{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Nachos",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/code/userprog/nachos",
      "args": ["-d", "s", "-rs", "12", "-x",  "test/makemultithreads"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/code",
      "environment": [],
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "make"
    }
  ]
}